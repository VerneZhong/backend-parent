<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.zxb.meetingfilm.film.dao.mapper.MoocActorTMapper">

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="com.zxb.meetingfilm.film.dao.entity.MoocActorT">
        <id column="UUID" property="uuid" />
        <result column="actor_name" property="actorName" />
        <result column="actor_img" property="actorImg" />
    </resultMap>
    <select id="describeActors" resultType="com.zxb.meetingfilm.film.controller.vo.DescribeActorsRespVO">
        select `uuid` as actorId, actor_name as actorName from mooc_actor_t
    </select>
</mapper>
